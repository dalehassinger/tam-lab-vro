    configurationElement.setAttributeWithKey('tenant', tenantIn, 'String');
    configurationElement.setAttributeWithKey('threadCount', threadCountIn, 'Number');
    configurationElement.setAttributeWithKey('vra7host', vra7hostIn, 'String');
