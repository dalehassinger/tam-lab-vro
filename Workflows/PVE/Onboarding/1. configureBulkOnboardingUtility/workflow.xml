    configurationElement.setAttributeWithKey('vra8host', vra8hostIn, 'String');
    configurationElement.setAttributeWithKey('vra8Password', vra8PasswordIn, 'SecureString');
    configurationElement.setAttributeWithKey('vroNodeIP', vroNodeIPIn, 'String');
    configurationElement.setAttributeWithKey('windowsCloudTemplateName', windowsCloudTemplateNameIn, 'String');
    configurationElement.setAttributeWithKey('vra8Username', vra8UsernameIn, 'String');
